{
  "name": "react-particle-image",
  "version": "1.0.0",
  "description": "Turn images into interactive particles",
  "author": "malerba118",
  "license": "MIT",
  "repository": "malerba118/react-particle-image",
  "keywords": [
    "particle",
    "image",
    "react"
  ],
  "main": "dist/index.js",
  "module": "dist/index.es.js",
  "jsnext:main": "dist/index.es.js",
  "types": "declaration/index.d.ts",
  "engines": {
    "node": ">=8",
    "npm": ">=5"
  },
  "scripts": {
    "test": "cross-env CI=1 react-scripts-ts test --env=jsdom",
    "test:watch": "react-scripts-ts test --env=jsdom",
    "lint": "tsc --noEmit && eslint 'src/**/*.{js,ts,tsx}' --quiet --fix",
    "build": "rollup -c && npm run declaration",
    "declaration": "rm -rf declaration && tsc -p tsconfig.declaration.json",
    "generate-docs": "typedoc",
    "start": "rollup -c -w",
    "prepare": "yarn run build",
    "predeploy": "npm run generate-docs",
    "deploy": "gh-pages --dotfiles -d docs",
    "storybook": "start-storybook -s ./public -p 6006",
    "build-storybook": "build-storybook"
  },
  "dependencies": {
    "lodash.flatmap": "^4.5.0",
    "lodash.isequal": "^4.5.0",
    "lodash.throttle": "^4.1.1"
  },
  "peerDependencies": {
    "react": "^16.8.0",
    "react-dom": "^16.8.0"
  },
  "devDependencies": {
    "@babel/preset-react": "^7.7.4",
    "@storybook/addon-actions": "^5.2.8",
    "@storybook/addon-knobs": "^5.3.9",
    "@storybook/addon-links": "^5.2.8",
    "@storybook/addons": "^5.2.8",
    "@storybook/react": "^5.2.8",
    "@svgr/rollup": "^2.4.1",
    "@types/enzyme": "^3.10.4",
    "@types/enzyme-adapter-react-16": "^1.0.5",
    "@types/jest": "^23.1.5",
    "@types/lodash.flatmap": "^4.5.6",
    "@types/lodash.isequal": "^4.5.5",
    "@types/lodash.throttle": "^4.1.6",
    "@types/react": "^16.3.13",
    "@types/react-dom": "^16.0.5",
    "@typescript-eslint/eslint-plugin": "^2.10.0",
    "@typescript-eslint/parser": "^2.10.0",
    "awesome-typescript-loader": "^5.2.1",
    "babel-core": "^6.26.3",
    "babel-loader": "^8.0.6",
    "babel-preset-react-app": "^9.1.0",
    "babel-runtime": "^6.26.0",
    "cross-env": "^5.1.4",
    "enzyme": "^3.10.0",
    "enzyme-adapter-react-16": "^1.15.1",
    "eslint": "^6.7.2",
    "gh-pages": "^1.2.0",
    "react": "^16.8.0",
    "react-dom": "^16.8.0",
    "react-fps-stats": "^0.1.2",
    "react-scripts-ts": "^2.16.0",
    "react-use": "^13.22.2",
    "rollup": "^0.62.0",
    "rollup-plugin-babel": "^3.0.7",
    "rollup-plugin-commonjs": "^9.1.3",
    "rollup-plugin-node-resolve": "^3.3.0",
    "rollup-plugin-peer-deps-external": "^2.2.0",
    "rollup-plugin-postcss": "^1.6.2",
    "rollup-plugin-typescript2": "^0.17.0",
    "rollup-plugin-url": "^1.4.0",
    "typedoc": "^0.16.9",
    "typedoc-plugin-no-inherit": "^1.1.10",
    "typedoc-plugin-nojekyll": "^1.0.1",
    "typescript": "^3.7.3"
  },
  "files": [
    "dist",
    "declaration"
  ]
}
